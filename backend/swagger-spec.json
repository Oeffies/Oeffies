{
  "openapi": "3.0.0",
  "paths": {
    "/app": {
      "get": {
        "operationId": "AppController_getHello",
        "summary": "returns a \"Hello World!\" message",
        "description": "Test enpoint to send a \"Hello World!\" message to frontend.",
        "parameters": [],
        "responses": {
          "200": {
            "description": "Returns a \"Hello World!\" message.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/HelloWorld"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad request.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BadRequest"
                }
              }
            }
          }
        },
        "tags": [
          "app"
        ]
      }
    },
    "/users": {
      "get": {
        "operationId": "UsersController_findAll",
        "summary": "returns all users",
        "description": "Get all users.",
        "parameters": [],
        "responses": {
          "200": {
            "description": "Return all Users as JSON array.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/User"
                  }
                }
              }
            }
          },
          "404": {
            "description": "Bad request.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BadRequest"
                }
              }
            }
          }
        },
        "tags": [
          "users"
        ]
      },
      "post": {
        "operationId": "UsersController_create",
        "summary": "creates a new user, returns the created on",
        "description": "Create a new user.",
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/User"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Returns the created user.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/User"
                }
              }
            }
          },
          "404": {
            "description": "Bad request.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BadRequest"
                }
              }
            }
          }
        },
        "tags": [
          "users"
        ]
      }
    },
    "/users/{index}": {
      "get": {
        "operationId": "UsersController_findOne",
        "summary": "returns a specific user",
        "description": "Get a specific user.",
        "parameters": [
          {
            "name": "index",
            "required": true,
            "in": "path",
            "description": "Users index",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Returns a User by index.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/User"
                }
              }
            }
          },
          "404": {
            "description": "User not found.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserNotFound"
                }
              }
            }
          }
        },
        "tags": [
          "users"
        ]
      },
      "patch": {
        "operationId": "UsersController_update",
        "summary": "updates a specific user",
        "description": "Update a specific user.",
        "parameters": [
          {
            "name": "index",
            "required": true,
            "in": "path",
            "description": "Users index",
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/User"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Returns the updated user.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/User"
                }
              }
            }
          },
          "404": {
            "description": "User not found.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserNotFound"
                }
              }
            }
          }
        },
        "tags": [
          "users"
        ]
      },
      "delete": {
        "operationId": "UsersController_remove",
        "summary": "deletes a specific user",
        "description": "Delete a specific user.",
        "parameters": [
          {
            "name": "index",
            "required": true,
            "in": "path",
            "description": "Users index",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Returns the deleted user.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/User"
                }
              }
            }
          },
          "404": {
            "description": "User not found.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserNotFound"
                }
              }
            }
          }
        },
        "tags": [
          "users"
        ]
      }
    },
    "/journeys": {
      "post": {
        "operationId": "JourneysController_planJourney",
        "summary": "Returns variants of planned journey.",
        "description": "Plan variants of a journey.",
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PlanJourneyDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Returns variants of planned journey.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/JourneyVariant"
                  }
                }
              }
            }
          }
        },
        "tags": [
          "journeys"
        ]
      }
    },
    "/journeys/refresh/{token}": {
      "get": {
        "operationId": "JourneysController_refreshJourney",
        "summary": "Returns refreshed data of a journey variant.",
        "description": "Refresh (receive updated data of) a journey variant that has been planned before.",
        "parameters": [
          {
            "name": "token",
            "required": true,
            "in": "path",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Returns refreshed journey variant.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/JourneyVariant"
                }
              }
            }
          },
          "400": {
            "description": "Refreshing journey variant is not available with endpoint being used.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RefreshJourneyNotAvailable"
                }
              }
            }
          }
        },
        "tags": [
          "journeys"
        ]
      }
    },
    "/journeys/location/{query}": {
      "get": {
        "operationId": "JourneysController_searchLocation",
        "summary": "Returns all locations matching the given query.",
        "description": "Search a location with the given query.",
        "parameters": [
          {
            "name": "query",
            "required": true,
            "in": "path",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Returns location results.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/JourneyLocation"
                  }
                }
              }
            }
          }
        },
        "tags": [
          "journeys"
        ]
      }
    }
  },
  "info": {
    "title": "NestJS Swagger",
    "description": "API description",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [
    {
      "url": "http://localhost:3000"
    }
  ],
  "components": {
    "schemas": {
      "HelloWorld": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          }
        },
        "required": [
          "message"
        ]
      },
      "BadRequest": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "object",
            "description": "Http status code",
            "example": 400
          },
          "message": {
            "type": "object",
            "description": "Error message",
            "example": "Bad request."
          }
        },
        "required": [
          "statusCode",
          "message"
        ]
      },
      "User": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Name of a User",
            "example": "John Smith"
          },
          "age": {
            "type": "number",
            "description": "Age of a User",
            "example": 45
          },
          "city": {
            "type": "string",
            "description": "City of a User",
            "example": "New York"
          }
        },
        "required": [
          "name",
          "age",
          "city"
        ]
      },
      "UserNotFound": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "object",
            "description": "Http status code",
            "example": 404
          },
          "message": {
            "type": "object",
            "description": "Error message",
            "example": "User not found."
          }
        },
        "required": [
          "statusCode",
          "message"
        ]
      },
      "JourneyStopStationIdDto": {
        "type": "object",
        "properties": {
          "stopStationId": {
            "type": "string",
            "description": "ID of a hafas stop or station."
          }
        },
        "required": [
          "stopStationId"
        ]
      },
      "JourneyUserLocationDto": {
        "type": "object",
        "properties": {
          "latitude": {
            "type": "number",
            "description": "Latitude of the user location."
          },
          "longitude": {
            "type": "number",
            "description": "Longitude of the user location."
          }
        },
        "required": [
          "latitude",
          "longitude"
        ]
      },
      "PlanJourneyDto": {
        "type": "object",
        "properties": {
          "from": {
            "description": "Location where the journey starts.",
            "oneOf": [
              {
                "$ref": "#/components/schemas/JourneyStopStationIdDto"
              },
              {
                "$ref": "#/components/schemas/JourneyUserLocationDto"
              }
            ]
          },
          "to": {
            "description": "Location where the journey ends.",
            "oneOf": [
              {
                "$ref": "#/components/schemas/JourneyStopStationIdDto"
              },
              {
                "$ref": "#/components/schemas/JourneyUserLocationDto"
              }
            ]
          }
        },
        "required": [
          "from",
          "to"
        ]
      },
      "JourneyVariant": {
        "type": "object",
        "properties": {
          "journey": {
            "type": "object",
            "description": "One variant of a planned journey (Friendly Public Transport Format, FPTF)."
          },
          "updatedAt": {
            "type": "number",
            "description": "Point of time the data of the journey variant has been fetched."
          }
        },
        "required": [
          "journey",
          "updatedAt"
        ]
      },
      "RefreshJourneyNotAvailable": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "object",
            "description": "Http status code",
            "example": 400
          },
          "message": {
            "type": "object",
            "description": "Error message",
            "example": "Refresh journey not available."
          }
        },
        "required": [
          "statusCode",
          "message"
        ]
      },
      "JourneyLocation": {
        "type": "object",
        "properties": {
          "location": {
            "type": "object",
            "description": "One location that can be a FPTF station, stop or location (FPTF = Friendly Public Transport Format)."
          }
        },
        "required": [
          "location"
        ]
      }
    }
  }
}
