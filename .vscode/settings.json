{
  "search.exclude": {
    "**/node_modules": true,
    "**/dist": true,
    "**/out": true
  },
  "git.branchProtection": [
    "main"
  ],
  "git.branchProtectionPrompt": "alwaysCommitToNewBranch",
  "git.pullBeforeCheckout": true,
  "files.insertFinalNewline": true,
  "editor.indentSize": "tabSize",
  "editor.tabSize": 2,
  "[typescript][typescriptreact][javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.formatOnSave": true
  },
  "eslint.workingDirectories": [
    "./frontend",
    "./backend",
    "./vrr_client",
    "./crawlers"
  ],
  "cSpell.words": [
    "Abfahrtszeit",
    "Alternativhaltestellen",
    "Änderungen",
    "angefahren",
    "Ankunftszeit",
    "Baumaßnahme",
    "begrenzt",
    "berücksichtigt",
    "beschriebenen",
    "Dauer",
    "Dorsten",
    "durch",
    "Einstiegshilfe",
    "elektronischen",
    "Enfieldstraße",
    "Erler",
    "Fahrplanauskunft",
    "Fahrradmitnahme",
    "Fahrzeuggebundene",
    "favorisierten",
    "Favoriten",
    "Fußpfad",
    "Gelsen",
    "Gladbeck",
    "Hafas",
    "Haltestelle",
    "Haltestellen",
    "Haltestellenausfälle",
    "Haltestellenausfällen",
    "Hegestraße",
    "Heuss",
    "hinzufügen",
    "ionicons",
    "Jetzt",
    "Keine",
    "kommt",
    "Linie",
    "Linien",
    "Löbe",
    "Merken",
    "möglich",
    "nicht",
    "Öffis",
    "Regio",
    "Regionalzug",
    "rollstuhltaugliches",
    "Routen",
    "Startpunkt",
    "Straße",
    "Stresemannstraße",
    "suchen",
    "Theodor",
    "Uhrzeit",
    "vorhanden",
    "Weiteres",
    "werden",
    "Zielpunkt"
  ],
  "editor.codeActionsOnSave": {
    "source.organizeImports": "always"
  }
}
