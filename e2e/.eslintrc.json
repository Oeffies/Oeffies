{
    "ignorePatterns": [
        "node_modules/"
    ],
    "root": true,
    "env": {
        "es6": true,
        "browser": true
    },
    "plugins": [
        "@typescript-eslint",
        "cypress"
    ],
    "extends": [
        "plugin:cypress/recommended",
        "../.eslintrc.json"
    ],
    "parserOptions": {
        "sourceType": "module",
        "project": [
            "tsconfig.json"
        ],
        "ecmaVersion": 6
    },
    "rules": {}
}
